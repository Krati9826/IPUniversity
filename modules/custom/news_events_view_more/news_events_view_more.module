<?php

/**
 * @file
 * Contains news_events_view_more.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function news_events_view_more_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the news_events_view_more module.
    case 'help.page.news_events_view_more':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Api module') . '</p>';
      return $output;

    default:
  }
}
